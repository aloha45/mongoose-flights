<%- include('../partials/header') %>

<body>
  <h1>Flight List</h1>
    <table>
      <thead>
        <tr>
          <th>Airline</th>
          <th>Airport</th>
          <th>Flight Details</th>
        </tr>
      </thead>
      <tbody>
        <% flights.forEach((f) => { %>
        <tr>
          <td><%= f.airline %></td>
          <td><%= f.airport %></td>
          <td><a href="/flights/<%= f._id %>">More Info Here</a></td>
        </tr>
    <% }) %>
    </tbody>
  </table><br>
  <a href="flights/new">Book a flight</a>

  <%- include('../partials/footer') %>